{
  "name": "dnscontrol-ts",
  "version": "1.0.0",
  "description": "Reproducible DNS record entries for zhr.one properties",
  "private": true,
  "scripts": {
    "build": "npm run lint && webpack",
    "preview": "run-script-os",
    "preview:default": "npm run build && cd out && ../lib/dnscontrol preview",
    "preview:win32": "npm run build && cd out && ..\\lib\\dnscontrol preview",
    "push": "run-script-os",
    "push:default": "npm run build && cd out && ../lib/dnscontrol push",
    "push:win32": "npm run build && cd out && ..\\lib\\dnscontrol push",
    "lint": "eslint --ext .ts ."
  },
  "author": "Vlad Zaharia",
  "license": "All Rights Reserved",
  "devDependencies": {
    "@types/node": "^13.7.7",
    "@types/webpack": "^4.41.7",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "cli-confirm": "^1.0.1",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.8.0",
    "eslint-config-standard-with-typescript": "^14.0.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "run-script-os": "^1.1.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }
}
