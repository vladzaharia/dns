{
  "name": "dnscontrol-ts",
  "version": "1.0.0",
  "description": "Reproducible DNS record entries for zhr.one properties",
  "private": true,
  "scripts": {
    "build": "npm run lint && webpack",
    "preview-ci": "run-script-os",
    "preview-ci:default": "cd out && ../lib/dnscontrol preview",
    "preview-ci:win32": "cd out && ..\\lib\\dnscontrol preview",
    "preview": "npm run build && npm run preview-ci",
    "push-ci": "run-script-os",
    "push-ci:default": "cd out && ../lib/dnscontrol push",
    "push-ci:win32": "cd out && ..\\lib\\dnscontrol push",
    "push": "npm run build && npm run push-ci",
    "lint": "eslint --ext .ts ."
  },
  "author": "Vlad Zaharia",
  "license": "All Rights Reserved",
  "devDependencies": {
    "@types/node": "^13.7.7",
    "@types/webpack": "^4.41.7",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "cli-confirm": "^1.0.1",
    "copy-webpack-plugin": "^6.1.1",
    "elliptic": ">=6.5.3",
    "eslint": "^6.8.0",
    "eslint-config-standard-with-typescript": "^14.0.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "run-script-os": "^1.1.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }
}
